public class Rect

{

double len;

float bre;

void setdata()

{
len=14.3;
bre=13;
System.out.println("area is");
System.out.println("length is :"+len);

System.out.println("breadth is :"+bre);

}

double getarea()

{

double area;

area=len*bre;
return area;

}

public static void main(String args[])

{

Rect obj=new Rect();

obj.setdata();

System.out.println("area is"+obj.getarea());

}

}
